#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int N, original, temp, digit, sum=0, count=0;
    scanf("%d", &N);
    original=N;
    temp=N;
    while(temp>0){
        count++;
        temp=temp/10;
    }
    temp=N;
    while(temp>0){
        digit=temp%10;
        sum+=pow(digit, count);
        temp=temp/10;
    }
    if(sum==original){
        printf("Armstrong\n");
    }else{
        printf("Not Armstrong\n");
    }
    return 0;
}
