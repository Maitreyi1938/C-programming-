#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int n;
    scanf("%d",&n);
    int arr[n];
    int even_count=0, odd_count=0;
    for(int i=0; i<n; i++){
        scanf("%d",&arr[i]);
        if(arr[i]%2==0){
            even_count++;
        }else{
            odd_count++;
        }
    }
    printf("%d ",even_count);
    printf("%d ",odd_count);
    return 0;
}
