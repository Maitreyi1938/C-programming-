#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int N, factorial=1;
    scanf("%d", &N);
    if(N<0){
        printf("Factorial is not defined for negative numbers\n");
    }else{
        for(int i=1; i<=N; i++){
            factorial = factorial*i;
        }
        printf("%d\n", factorial);
    }
    
    return 0;
}
