#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int N, temp, digits=0, first, last, pow=1;
    scanf("%d", &N);
    temp=N;
    last=temp%10;
    while(temp>0){
        first=temp%10;
        temp=temp/10;
        digits++;
    }
    if(digits==1){
        printf("%d\n", N);
        return 0;
    }
    for(int i=1; i<digits; i++){
        pow*=10;
    }
    int middle=(N%pow)/10;
    int n=last*pow+middle*10+first;
    printf("%d\n", n);
    return 0;
}
